help:  ## Show help
	@grep -E '^[.a-zA-Z_-]+:.*?## .*$$' $(MAKEFILE_LIST) | awk 'BEGIN {FS = ":.*?## "}; {printf "\033[36m%-30s\033[0m %s\n", $$1, $$2}'

clean: ## Clean autogenerated files
	rm -rf dist
	find . -type f -name "*.DS_Store" -ls -delete
	find . | grep -E "(__pycache__|\.pyc|\.pyo)" | xargs rm -rf
	find . | grep -E ".pytest_cache" | xargs rm -rf
	find . | grep -E ".ipynb_checkpoints" | xargs rm -rf
	rm -f .coverage

clean-logs: ## Clean logs
	rm -r logs/**

vanilla: ## Run vanilla experiment
	python spider/vanilla.py

in-context-learning: ## Run in-context learning experiment
	python spider/in_context_learning.py -m prompts=default,icl-1,icl-2,icl-3,icl-4 pretrained_model_name_or_path=mistralai/Mixtral-8x7B-Instruct-v0.1
